<?xml version="1.0" encoding="utf-8" ?>
<root>
	<filters>
		<!-- DEPTH 1 (inside baseFilter) - - - - - - - - -->
		<tileType>
			<defaultChildTargetType>Byte</defaultChildTargetType>
			<defaultChildTargetPos>1</defaultChildTargetPos>
			
			<targetValues>
				<value>3</value>
			</targetValues>
			
			<elementSetName>tileTypeBase</elementSetName>
			<childrenNames>
				<name>tileCrossMarkable</name>
				<name>tileMerge</name>
			</childrenNames>
			<dataSize>8</dataSize>
		</tileType>

		<!-- MARKER BRANCHES -->
		<tileCrossMarkable>
			<targetValues>
				<value>2</value>
				<value>3</value>
			</targetValues>
			<markers>
				<marker>
					<type>cross</type>
					<data>
						<XValueType>Part</XValueType>
						<XValuePos>33</XValuePos>
						<XValueLength>6</XValueLength>
						<YValueType>Part</YValueType>
						<YValuePos>39</YValuePos>
						<YValueLength>6</YValueLength>
					</data>
				</marker>
			</markers>
			<childrenNames>
				<name>tileMerge</name>
			</childrenNames>
		</tileCrossMarkable>

		<!-- BRANCH MERGE-->
		<tileMerge>
			<defaultChildTargetType>Byte</defaultChildTargetType>
			<defaultChildTargetPos>1</defaultChildTargetPos>
			<childrenNames>
				<name>chest</name>
				<name>unknownTileType</name>
			</childrenNames>
		</tileMerge>

		<!-- DEPTH 2 (inside tileType) - - - - - - - - - -->
		<chest>
			<!-- Check byte 3 which is the type of item held -->
			<defaultChildTargetType>Byte</defaultChildTargetType>
			<defaultChildTargetPos>3</defaultChildTargetPos>
			<targetValues>
				<value>2</value>
				<value>3</value>
			</targetValues>
			<elementSetName>chestBase</elementSetName>
			<childrenNames>
				<name>chestKinstone</name>
				<name>chestDefault</name>
			</childrenNames>
		</chest>

		<unknownTileType>
			<elementSetName>halfRepresentation</elementSetName>
		</unknownTileType>
		
		<!-- DEPTH 3 (inside chest) - - - - - - - - - - -->
		<chestKinstone>
			<targetValues>
				<value>0x5C</value>
			</targetValues>
			<elementSetName>kinstoneChest</elementSetName>
		</chestKinstone>

		<chestDefault>
			<elementSetName>defaultChest</elementSetName>
		</chestDefault>

	</filters>
</root>